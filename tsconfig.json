{
  "compilerOptions": {
    "baseUrl": "",
    "paths": {
      "@ui/*": [
        "src/common/resources/client/ui/library/*"
      ],
      "@common/*": [
        "src/common/resources/client/*"
      ],
      "@app/*": [
        "src/*"
      ]
    },
    "target": "ESNext",
    "useDefineForClassFields": true,
    "lib": [
      "DOM",
      "DOM.Iterable",
      "ESNext"
    ],
    "allowJs": false,
    "skipLibCheck": false,
    "esModuleInterop": false,
    "allowSyntheticDefaultImports": true,
    "strict": true,
    "forceConsistentCasingInFileNames": true,
    "module": "ESNext",
    "moduleResolution": "Node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "noEmit": true,
    "jsx": "react-jsx",
    "outDir": "dist/types",
    "declaration": true,
    "declarationMap": true
  },
  "include": [
    "./src"
  ],
  "references": [
    {
      "path": "./tsconfig.node.json"
    }
  ]
}
